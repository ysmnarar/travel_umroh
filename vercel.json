{
     "version": 2,
     "builds": [
       {
         "src": "./public",
         "use": "@vercel/static-build",
         "config": {
           "indexDocument": "index.php"
         }
       }
     ],
     "routes": [
       {
         "src": "/",
         "dest": "/index.php"
       }
     ]
   }